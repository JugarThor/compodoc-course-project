{
  "name": "blog-angular",
  "version": "0.0.0",
  "description": "Compodoc Course template to complete course step by step. Project in Angular 8",
  "scripts": {
    "ng": "ng",
    "start": "ng serve -o --port 4500",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "prod": "ng build --prod --base-href=\"./\"",
    "compodoc": "npx compodoc -p tsconfig.doc.json -d docs -o -s",
    "compodoc.coverage": "npx compodoc -p tsconfig.doc.json -d docs -o -s -w",
    "material.theme": "npx compodoc -p tsconfig.doc.json -d docs -o -s --theme material",
    "laravel.theme": "npx compodoc -p tsconfig.doc.json -d docs -o -s --theme laravel",
    "original.theme": "npx compodoc -p tsconfig.doc.json -d docs -o -s --theme original",
    "vagrant.theme": "npx compodoc -p tsconfig.doc.json -d docs -o -s --theme vagrant",
    "postmark.theme": "npx compodoc -p tsconfig.doc.json -d docs -o -s --theme postmark",
    "readthedocs.theme": "npx compodoc -p tsconfig.doc.json -d docs -o -s --theme readthedocs",
    "stripe.theme": "npx compodoc -p tsconfig.doc.json -d docs -o -s --theme stripe",
    "aditional": "npx compodoc -p tsconfig.doc.json -d docs -o -s --includes ./aditional -w",
    "custom.title.favicon": "npx compodoc -p tsconfig.doc.json -d docs -o -s --includes ./aditional -w --customFavicon ./book.ico --name \"MiDocumentación\"",
    "custom.style": "npx compodoc -p tsconfig.doc.json -d docs -o -s --includes ./aditional -w --customFavicon ./book.ico --name \"MiDocumentación\" -y ./css/ --hideGenerator",
    "tips.compodoc": "npx compodoc -p tsconfig.doc.json -d docs -o -s --includes ./aditional -w --includesName \"MiDocumentacionPlus\"",
    "compodoc-build": "npx compodoc -p tsconfig.doc.json -d documentation --includes ./aditional --customFavicon ./book.ico"

  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.0.1",
    "@angular/common": "~9.0.1",
    "@angular/compiler": "~9.0.1",
    "@angular/core": "~9.0.1",
    "@angular/forms": "~9.0.1",
    "@angular/platform-browser": "~9.0.1",
    "@angular/platform-browser-dynamic": "~9.0.1",
    "@angular/router": "~9.0.1",
    "core-js": "^2.5.4",
    "rxjs": "~6.5.2",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.900.2",
    "@angular/cli": "~9.0.2",
    "@angular/compiler-cli": "~9.0.1",
    "@angular/language-service": "~9.0.1",
    "@compodoc/compodoc": "^1.1.14",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.7.5"
  }
}
